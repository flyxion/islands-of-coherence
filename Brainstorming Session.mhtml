<span class="line"><span style="color: rgb(78, 201, 176);">- \frac{c_</span><span style="color: rgb(86, 156, 214);">\Phi</span><span style="color: rgb(78, 201, 176);">^</span><span style="color: rgb(181, 206, 168);">2</span><span style="color: rgb(78, 201, 176);">}{</span><span style="color: rgb(181, 206, 168);">2</span><span style="color: rgb(78, 201, 176);">} |D_i </span><span style="color: rgb(86, 156, 214);">\Phi</span><span style="color: rgb(78, 201, 176);">|^</span><span style="color: rgb(181, 206, 168);">2</span></span>
<span class="line"><span style="color: rgb(78, 201, 176);">+ \frac{</span><span style="color: rgb(86, 156, 214);">\lambda</span><span style="color: rgb(78, 201, 176);">_v}{</span><span style="color: rgb(181, 206, 168);">2</span><span style="color: rgb(78, 201, 176);">} \left( \bm{v}^</span><span style="color: rgb(86, 156, 214);">\mu</span><span style="color: rgb(78, 201, 176);"> - \frac{</span><span style="color: rgb(181, 206, 168);">1</span><span style="color: rgb(78, 201, 176);">}{</span><span style="color: rgb(86, 156, 214);">\lambda</span><span style="color: rgb(78, 201, 176);">_v} A^</span><span style="color: rgb(86, 156, 214);">\mu</span><span style="color: rgb(78, 201, 176);"> \right)^</span><span style="color: rgb(181, 206, 168);">2</span></span>
<span class="line"><span style="color: rgb(78, 201, 176);">- U(S) </span><span style="color: rgb(197, 134, 192);">\\</span></span>
<span class="line"><span style="color: rgb(197, 134, 192);">&amp;</span><span style="color: rgb(78, 201, 176);">\quad - </span><span style="color: rgb(86, 156, 214);">\kappa</span><span style="color: rgb(220, 220, 170);"> \,</span><span style="color: rgb(86, 156, 214);"> \Phi</span><span style="color: rgb(78, 201, 176);">^* (D_</span><span style="color: rgb(86, 156, 214);">\mu</span><span style="color: rgb(78, 201, 176);"> \bm{v}^</span><span style="color: rgb(86, 156, 214);">\mu</span><span style="color: rgb(78, 201, 176);">)</span></span>
<span class="line"><span style="color: rgb(78, 201, 176);">+ </span><span style="color: rgb(86, 156, 214);">\beta</span><span style="color: rgb(220, 220, 170);"> \,</span><span style="color: rgb(78, 201, 176);"> S </span><span style="color: rgb(220, 220, 170);">\,</span><span style="color: rgb(78, 201, 176);"> (D_</span><span style="color: rgb(86, 156, 214);">\mu</span><span style="color: rgb(78, 201, 176);"> \bm{v}^</span><span style="color: rgb(86, 156, 214);">\mu</span><span style="color: rgb(78, 201, 176);">)</span></span>
<span class="line"><span style="color: rgb(78, 201, 176);">- \frac{</span><span style="color: rgb(181, 206, 168);">1</span><span style="color: rgb(78, 201, 176);">}{</span><span style="color: rgb(181, 206, 168);">4</span><span style="color: rgb(78, 201, 176);">} F_{</span><span style="color: rgb(86, 156, 214);">\mu\nu</span><span style="color: rgb(78, 201, 176);">} F^{</span><span style="color: rgb(86, 156, 214);">\mu\nu</span><span style="color: rgb(78, 201, 176);">},</span></span>
<span class="line"><span style="color: rgb(220, 220, 170);">\end</span><span style="color: rgb(78, 201, 176);">{</span><span style="color: rgb(156, 220, 254);">aligned</span><span style="color: rgb(78, 201, 176);">}</span></span>
<span class="line"><span style="color: rgb(220, 220, 170);">\end</span><span style="color: rgb(230, 230, 230);">{</span><span style="color: rgb(156, 220, 254);">equation</span><span style="color: rgb(230, 230, 230);">}</span></span>
<span class="line"></span>
<span class="line"><span style="color: rgb(230, 230, 230);">with covariant derivatives</span></span>
<span class="line"><span style="color: rgb(220, 220, 170);">\begin</span><span style="color: rgb(230, 230, 230);">{</span><span style="color: rgb(156, 220, 254);">equation</span><span style="color: rgb(230, 230, 230);">}</span></span>
<span class="line"><span style="color: rgb(78, 201, 176);">D_</span><span style="color: rgb(86, 156, 214);">\mu</span><span style="color: rgb(86, 156, 214);"> \Phi</span><span style="color: rgb(78, 201, 176);"> = (</span><span style="color: rgb(86, 156, 214);">\partial</span><span style="color: rgb(78, 201, 176);">_</span><span style="color: rgb(86, 156, 214);">\mu</span><span style="color: rgb(78, 201, 176);"> - i A_</span><span style="color: rgb(86, 156, 214);">\mu</span><span style="color: rgb(78, 201, 176);">) </span><span style="color: rgb(86, 156, 214);">\Phi</span><span style="color: rgb(78, 201, 176);">, \qquad</span></span>
<span class="line"><span style="color: rgb(78, 201, 176);">D_</span><span style="color: rgb(86, 156, 214);">\mu</span><span style="color: rgb(78, 201, 176);"> \bm{v}^</span><span style="color: rgb(86, 156, 214);">\nu</span><span style="color: rgb(78, 201, 176);"> = </span><span style="color: rgb(86, 156, 214);">\partial</span><span style="color: rgb(78, 201, 176);">_</span><span style="color: rgb(86, 156, 214);">\mu</span><span style="color: rgb(78, 201, 176);"> \bm{v}^</span><span style="color: rgb(86, 156, 214);">\nu</span><span style="color: rgb(78, 201, 176);"> + </span><span style="color: rgb(86, 156, 214);">\Gamma</span><span style="color: rgb(78, 201, 176);">^</span><span style="color: rgb(86, 156, 214);">\nu</span><span style="color: rgb(78, 201, 176);">_{</span><span style="color: rgb(86, 156, 214);">\mu\lambda</span><span style="color: rgb(78, 201, 176);">} \bm{v}^</span><span style="color: rgb(86, 156, 214);">\lambda</span><span style="color: rgb(78, 201, 176);">,</span></span>
